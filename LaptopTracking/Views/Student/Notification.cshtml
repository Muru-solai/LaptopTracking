@model IEnumerable<UserBreachLog>
@{
    Layout = "_LayoutDashBoard";
    ViewData["IndexInfo"] = "Display all the App logs";
}
<script src="https://code.jquery.com/jquery-1.9.1.min.js"></script>
<script>
    $(document).ready(function () {

        $("#btnSubmit").click(function (e) {
            e.preventDefault();
            $.ajax({
                type: "POST",
                url: "/Student/SearchAppData/",
                data: {
                    CDate: $("#datePicker").val()
                },
                success: function (customers) {
                    var table = $("#example1");
                    table.find("tr:not(:first:last)").remove();
                    $.each(customers, function (i, customer) {
                        var table = $("#example1");
                        var row = table[0].insertRow(-1);
                        $(row).append("<td />");
                        $(row).find("td").eq(0).html(customer.iTS_Number);
                        $(row).append("<td />");
                        $(row).find("td").eq(1).html(customer.userCode);
                        $(row).append("<td />");
                        $(row).find("td").eq(2).html(customer.fullname);
                        $(row).append("<td />");
                        $(row).find("td").eq(3).html(customer.iP_Address);
                        $(row).append("<td />");
                        $(row).find("td").eq(4).html(customer.laptop_Model);
                          $(row).append("<td />");
                        $(row).find("td").eq(5).html(customer.Host);
                            $(row).append("<td />");
                        $(row).find("td").eq(6).html(customer.times);
                    });
                },
                error: function (result) {
                }
            });
        });
    });
</script>
<!-- Content Header (Page header) -->
<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-12">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="../Student/Index">Home</a></li>
                    <li class="breadcrumb-item active">Notification Dashboard</li>
                </ol>
            </div><!-- /.col -->
        </div><!-- /.row -->
    </div><!-- /.container-fluid -->
</div>
<!-- /.content-header -->
<div class="row">
    <div class="col-md-9">

        <div class="box box-primary">
            <div class="card">
                <div class="card-header">
                    <h1 class="card-title">
                        <b>
                            <h4><i class="fas fa-bell"></i> Notification Dashboard</h4>
                        </b>
                    </h1>
                </div>
                <!-- /.card-header -->
                <div class="card-body">
                    <table id="example1" class="table table-bordered table-striped">
                        <thead>
                            <tr>
                                <th>
                                    @Html.DisplayNameFor(model => model.ITS_Number)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.UserCode)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Fullname)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.IP_Address)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Laptop_Model)
                                </th>
                                   <th>
                                    @Html.DisplayNameFor(model => model.Host)
                                </th>
                                  <th>
                                    @Html.DisplayNameFor(model => model.times)
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                    <tr>
                                        <td>
                                        @Html.DisplayFor(modelItem => item.ITS_Number)
                                        </td>
                                        <td>
                                        @Html.DisplayFor(modelItem => item.UserCode)
                                        </td>
                                        <td>
                                        @Html.DisplayFor(modelItem => item.Fullname)
                                        </td>
                                        <td>
                                        @Html.DisplayFor(modelItem => item.IP_Address)
                                        </td>
                                        <td>
                                        @Html.DisplayFor(modelItem => item.Laptop_Model)
                                        </td>
                                          <td>
                                        @Html.DisplayFor(modelItem => item.Host)
                                        </td>
                                              <td>
                                        @Html.DisplayFor(modelItem => item.times)
                                        </td>
                                    </tr>
                            }
                        </tbody>
                        <tfoot>
                            <tr>
                                <th>
                                    @Html.DisplayNameFor(model => model.ITS_Number)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.UserCode)
                                </th>

                                <th>
                                    @Html.DisplayNameFor(model => model.Fullname)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.IP_Address)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Laptop_Model)
                                </th>
                                 <th>
                                    @Html.DisplayNameFor(model => model.Host)
                                </th>
                                 <th>
                                    @Html.DisplayNameFor(model => model.times)
                                </th>
                            </tr>
                        </tfoot>
                    </table>
                </div>
                <!-- /.card-body -->
            </div>
            <!-- /.card -->
            <!-- /.box-body -->
            <div class="box-footer no-padding">
            </div>
        </div>
    </div>
    <!-- /.col -->
</div>
<!-- /.row -->
